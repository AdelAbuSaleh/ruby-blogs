<p id="notice"><%= notice %></p>

<h1>V1 Sessions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v1_sessions.each do |v1_session| %>
      <tr>
        <td><%= link_to 'Show', v1_session %></td>
        <td><%= link_to 'Edit', edit_v1_session_path(v1_session) %></td>
        <td><%= link_to 'Destroy', v1_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

